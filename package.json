{
  "name": "gatsby-transformer-estates",
  "version": "0.0.1",
  "description": "A gatsby transformer plugin to sanitize data coming from gatsby-transformer-estates plugin",
  "author": {
    "name": "Johann Roehl",
    "email": "mail@johannroehl.de",
    "url": "https://johannroehl.de"
  },
  "bugs": {
    "url": "http://github.com/jroehl/gatsby-transformer-estates/issues"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore __tests__",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-transformer-plugin",
    "immobilienscout24",
    "is24",
    "flowfact",
    "estates"
  ],
  "homepage": "http://github.com/jroehl/gatsby-transformer-estates#readme",
  "repository": "http://github.com/jroehl/gatsby-transformer-estates",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "moment": "^2.22.0",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5"
  },
  "devDependencies": {
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "semantic-release": "^15.1.5",
    "travis-deploy-once": "^4.4.1"
  },
  "babel": {
    "presets": [
      "env",
      "stage-0"
    ]
  }
